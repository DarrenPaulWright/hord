{
	"name": "hord",
	"version": "0.2.1",
	"description": "A data storage and manipulation library for javascript",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs-readme": "jsdoc2md src/index.js > README.md --template docs.hbs --partial scope.hbs",
		"docs-list": "jsdoc2md src/List.js > docs/List.md --template docs.hbs",
		"docs": "npm run docs-readme && npm run docs-list"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/hord.git"
	},
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/hord/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/hord#readme",
	"keywords": [
		"collections",
		"indexeddb",
		"dexie",
		"mongo",
		"mongodb"
	],
	"author": "Darren Paul Wright <Darren@DarrenPaulWright.com>",
	"license": "MIT",
	"dependencies": {
		"default-compare": "^1.0.0",
		"type-enforcer": "^0.4.3"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.6",
		"babel-plugin-istanbul": "^5.1.4",
		"chai": "^4.2.0",
		"eslint": "^5.16.0",
		"eslint-loader": "^2.1.2",
		"eslint-plugin-babel": "^5.3.0",
		"jsdoc-to-markdown": "^5.0.0",
		"karma": "^4.1.0",
		"karma-brief-reporter": "0.1.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.1.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^6.1.4",
		"test-runner-config": "^0.5.0",
		"wallaby-webpack": "^3.9.15",
		"webpack": "^4.33.0"
	}
}
