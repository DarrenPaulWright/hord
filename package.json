		"@babel/preset-env": "^7.3.1",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-istanbul": "^5.1.0",
		"chai": "^4.2.0",
		"eslint": "^5.12.1",
		"eslint-loader": "^2.1.1",
