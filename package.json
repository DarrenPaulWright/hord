{
	"name": "hord",
	"version": "0.2.4",
	"description": "A data storage and manipulation library for javascript",
	"main": "src/index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs-readme": "jsdoc2md src/index.js > README.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-list": "jsdoc2md src/List.js > docs/List.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-schema": "jsdoc2md src/Schema/Schema.js > docs/Schema.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-model": "jsdoc2md src/Model.js > docs/Model.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs-collection": "jsdoc2md src/Collection.js > docs/Collection.md -t docs/docs.hbs --plugin dmd-readable -d 3",
		"docs": "npm run docs-readme && npm run docs-list && npm run docs-schema && npm run docs-model && npm run docs-collection"
	},
