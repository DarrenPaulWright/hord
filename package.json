		"jsdoc-to-markdown": "^4.0.1",
		"karma": "^3.1.4",
		"karma-brief-reporter": "0.0.7",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage": "^1.1.2",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.1.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"test-runner-config": "^0.5.0",
		"wallaby-webpack": "^3.9.13",
		"webpack": "^4.29.0"
